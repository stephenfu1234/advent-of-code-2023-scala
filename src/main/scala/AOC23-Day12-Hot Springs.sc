def findIdxOfWildcards(input: String) : List[Int] =
  input.zipWithIndex.filter(pair => pair._1 == '?').map(pair => pair._2).toList

def findWildcardCombinations(wildcardIdx: List[Int]): List[List[Int]] =
  wildcardIdx.toSet[Int].subsets.map(_.toList).toList

def findArrangements(input: String, wildcardIdx: List[Int], wildcardCombinations: List[List[Int]], mask: List[Int]) : Int =
  val defaultedInput = input.replace('?', '#')

  val arrangements = wildcardCombinations.map(combo =>
    // update the input string based on index combo
    combo.foldLeft(defaultedInput)((s, i) => s.updated(i, '.')).split('.').filter(_.nonEmpty)
  ).filter(_.length == mask.size)

  println(f"\ntotal variations to try ${arrangements.size} for $input")

  val matchingArrangements = arrangements.map(arrangement =>
    // then validate if it matches the mask
//    println(f"arrangement raw ${arrangement.length}")
//    println(f"mask size ${mask.size}")
    val sizes = arrangement.toList.map(_.length)
//    println(f"\narrangement to test: ${arrangement.mkString("Array(", ", ", ")")}")
//    println(f"arrangement mask $sizes is being compared to $mask")
    if (sizes == mask)
//      println("**** matching arrangement **** ")
      1
    else
      0
  )

//  println()
//  println(matchingArrangements)

  matchingArrangements.sum

def parse(line: String): (String, List[Int]) =
  val (s"$input $mask") = line: @unchecked
  (input, mask.split(',').map(_.toInt).toList)

def solution(input: String): Int =
  val reports = input.linesIterator.map(parse)
    .map{
    case (input, mask) =>
      val wildcardIdx = findIdxOfWildcards(input)
      val wildcardCombinations = findWildcardCombinations(wildcardIdx)
      findArrangements(input, wildcardIdx, wildcardCombinations, mask)
  }.toList
  println(reports)
  reports.sum

