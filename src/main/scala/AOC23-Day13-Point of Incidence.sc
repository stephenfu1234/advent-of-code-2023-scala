def LCSubStr(str1: Array[Char], str2: Array[Char], N: Int, M: Int) = {
  val LCStuff = Array.ofDim[Int](N + 1, M + 1)

  var mx = 0
  for (i <- 0 to N) {
    for (j <- 0 to M) {
      if (i == 0 || j == 0) LCStuff(i)(j) = 0
      else if (str1(i - 1) == str2(j - 1)) {
        LCStuff(i)(j) = LCStuff(i - 1)(j - 1) + 1
        mx = Integer.max(mx, LCStuff(i)(j))
      }
      else LCStuff(i)(j) = 0
    }
  }
  mx
}

