def solution(input: String) =
  val scores = input.split("\n\n").zipWithIndex.map {
    case (pattern, idx) =>
      println(f"Pattern ${idx+1}")
      calculateScoreForReflectionPoint(pattern)
  }.toList

  println("Scores: " + scores.mkString("Array(", ", ", ")"))
  println(f"Total score: ${scores.sum}")




