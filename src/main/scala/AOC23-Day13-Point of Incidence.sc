def calculateScoreForReflectionPoint(input: String): Int =
  val rows = input.linesIterator.toList
  println("NORMAL PATTERN")
  println(rows.foreach(println))
  println()

  val vertifcalReflections = input.linesIterator.map( row =>
    val lcs = longestCommonSubstring(row, row.reverse)
    val (reflectionIndex, reflectionWidth) = reflectionPoint(row, lcs)
    println(f"vertical reflection at col $reflectionIndex with width $reflectionWidth $lcs")
    (reflectionIndex, reflectionWidth)
  ).toList
  println(vertifcalReflections)
  println(vertifcalReflections.distinct.map(x => (x, vertifcalReflections.count(_ == x))))

  // transpose to find horizontal reflection using same technique.  Its easier to scan a row than a column
  println("TRANSPOSING")

  println(rows.transpose.map(_.mkString("")).foreach(println))
  println()
  val horizontalReflections = input.linesIterator.toList.transpose.map(_.mkString("")).map(col =>
    val lcs = longestCommonSubstring(col, col.reverse)
    val (reflectionIndex, reflectionWidth) = reflectionPoint(col, lcs)
    println(col + " " + col.reverse)
    println(f"horizontal reflection at col $reflectionIndex with width $reflectionWidth $lcs")
    (reflectionIndex, reflectionWidth)
  ).toList
  println(horizontalReflections)
  println(horizontalReflections.distinct.map(x => (x, horizontalReflections.count(_ == x))))

  // then take the largest reflection that touches the edge

  1



